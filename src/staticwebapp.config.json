{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*.js",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*.html",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/login",
      "rewrite": "/.auth/login/aad"
    }
  ]
}
